<%= form_for_jqm(@calendar) do |f| %>

  <%= render partial: 'shared/error_messages', object: f.object %>

  <!-- TBD:  replace person with name, talk with summary -->
  <%= f.number_field :person_id, placeholder: :person_id %>
  <%= f.number_field :talk_id, placeholder: :talk_id %>
  <%= f.text_field :name, placeholder: :name %>
  <%= f.text_area :description, placeholder: :description %>

  <%= render partial: 'shared/submit_button', :object => f %>

<% end %>
