<%= form_for_jqm(@faq) do |f| %>

  <%= render partial: 'shared/error_messages', object: @faq %>

  <%= f.text_area :question, placeholder: :question, value: '?' %>

  <%= f.text_area :answer, placeholder: :answer, value: '!' %>

  <%= f.submit %>

  <%= render partial: 'shared/delete_button', object: @faq %>

<% end %>
