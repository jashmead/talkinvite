<%= form_for_jqm(@faq) do |f| %>

  <%= render partial: 'shared/error_messages', object: f.object %>

  <%= f.text_area :question, placeholder: :question, required: true %>

  <%= f.text_area :answer, placeholder: :answer, required: true %>

  <%= render partial: 'shared/submit_button', :object => f %>

<% end %>
