<%= form_for_jqm(@credit) do |f| %>

  <%= render partial: 'shared/error_messages', object: f.object %>

  <%= f.text_field :name, placeholder: :name, required: true %>

  <%= f.text_area :description, placeholder: :description, required: true %>

  <% logger.debug("credits/_form: f.object: #{f.object.inspect}") %>

  <%= render partial: 'shared/submit_button', :object => f %>

<% end %>
