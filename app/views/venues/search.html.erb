<% provide(:title, 'Search for Venues') %>
<% provide(:data_role, 'dialog') %>

<%= form_tag("/venues/found", method: "get") do %>
  <%= label_tag(:search_q, "Search for:") %>
  <%= search_field(:search, :q) %>
  <%= submit_tag("Search", { 'name' => 'button', 'data-inline' => true } ) %>
  <br/>
  <%= submit_tag("Add Venue", { 'name' => 'button', 'data-inline' => true } ) %>
<% end %>
