<%= form_for_jqm(@map) do |f| %>

  <%= render partial: 'shared/error_messages', object: f.object %>

  <%= f.text_field :name, placeholder: :name%>
  <%= f.text_field :description, placeholder: :description %>
  <%= f.text_area :geometry, placeholder: :geometry %>
  <%= f.text_area :settings, placeholder: :settings %>
  <%= f.text_area :history, placeholder: :history, readonly: true %>

  <%= f.label :description %><br>

<% end %>
