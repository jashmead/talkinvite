<% provide(:title, "Edit Person") %>
<% logger.debug("views/people/edit: @title: #{@title}, @person: #{@person.inspect}") %>
<%= form_for_jqm(@person) do |f| %>

  <%= render partial: 'shared/error_messages', object: f.object %>

  <%= render partial: 'form_core', object: f %>

  <%= render partial: 'password', object: f %>

  <%= render partial: 'person_save', object: f %>

<% end %>
